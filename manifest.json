{ "manifest_version": 2
, "name": "FB-GC"
, "description": "Applying a garbage collection when accessing some pages at FB"
, "version": "1.0"
, "permissions":
  [ "tabs"
  ]
, "background":
  { "scripts": ["bg.js"]
  }
, "content_scripts":
  [ { "matches": ["https://www.facebook.com/*"]
    , "js": ["content.js"]
    }
  ]
}
